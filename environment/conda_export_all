#conda env export -n SimPEG >~/Admin/Anaconda/SimPEG.yml
# conda env export -n Fenics >~/Admin/Anaconda/Fenics.yml
#conda env export -n EarthEngine >~/Admin/Anaconda/EarthEngine.yml
