conda list  -n base > ~/Admin/Anaconda/base.txt
conda list  -n EM >~/Admin/Anaconda/EM.txt
#conda list  -n SimPEG >~/Admin/Anaconda/SimPEG.txt
#conda list  -n Fenics >~/Admin/Anaconda/Fenics.txt
#conda list  -n EarthEngine >~/Admin/Anaconda/EarthEngine.txt
